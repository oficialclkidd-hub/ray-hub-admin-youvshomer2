local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

local Window = WindUI:CreateWindow({
    Title = "Ray hub | you vs homer v2",
    Icon = "rbxthumb://type=Asset&id=75069275471141&w=420&h=420", 
    Author = "by Fx_shura and denolk and scripts073",
    Folder = "MyRayHub",
    Size = UDim2.fromOffset(580, 460),
    Transparent = true,
    Theme = "Red", 
    Accent = Color3.fromRGB(255, 0, 0), 
    Resizable = true,
    User = {
        Enabled = true,
        Anonymous = false,
    },
})

-- TABS
local MainTab = Window:Tab({ Title = "Principal", Icon = "user" }) 
local SpeedTab = Window:Tab({ Title = "Velocidade", Icon = "zap" })
local ScriptsTab = Window:Tab({ Title = "Scripts", Icon = "code" })
local KeysTab = Window:Tab({ Title = "Keys", Icon = "key" })
local FakeTab = Window:Tab({ Title = "Fake", Icon = "ghost" })
local ESPTab = Window:Tab({ Title = "Visual/ESP", Icon = "eye" })
local MiscTab = Window:Tab({ Title = "Discord e etc", Icon = "link" })

--- SE√á√ÉO PRINCIPAL ---

MainTab:Toggle({
    Title = "üõ°Ô∏è God Mode (Semi)",
    Default = false,
    Callback = function(Value)
        local hum = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if Value and hum then
            hum.MaxHealth = math.huge
            hum.Health = math.huge
        else
            WindUI:Notify({Title = "Aviso", Content = "D√™ reset para desativar!", Duration = 3})
        end
    end
})

MainTab:Toggle({
    Title = "üëª Invis√≠vel",
    Default = false,
    Callback = function(Value)
        local char = game.Players.LocalPlayer.Character
        for _, part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") or part:IsA("Decal") then
                part.Transparency = Value and 1 or 0
            end
        end
    end
})

MainTab:Toggle({
    Title = "ü¶ò Infinite Jump",
    Default = false,
    Callback = function(Value)
        _G.InfJump = Value
        game:GetService("UserInputService").JumpRequest:Connect(function()
            if _G.InfJump then
                game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
            end
        end)
    end
})

MainTab:Toggle({
    Title = "üß± Wall Hop (Plataforma)",
    Default = false,
    Callback = function(Value)
        _G.WallHop = Value
        game:GetService("UserInputService").JumpRequest:Connect(function()
            if _G.WallHop then
                local char = game.Players.LocalPlayer.Character
                local hrp = char:FindFirstChild("HumanoidRootPart")
                if hrp then
                    local plate = Instance.new("Part")
                    plate.Size = Vector3.new(8, 1, 8)
                    plate.Transparency = 1 
                    plate.Anchored = true
                    plate.CanCollide = true
                    plate.Parent = workspace
                    plate.CFrame = hrp.CFrame * CFrame.new(0, -3.7, 0)
                    game.Debris:AddItem(plate, 0.1)
                end
            end
        end)
    end
})

--- SE√á√ÉO VELOCIDADE ---

SpeedTab:Toggle({
    Title = "üöÄ FPS Booster (Speed 26)",
    Default = false,
    Callback = function(Value)
        if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value and 26 or 16
        end
    end
})

SpeedTab:Toggle({
    Title = "üèÉ Speed 1.3x (Speed 21)",
    Default = false,
    Callback = function(Value)
        if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value and 21 or 16
        end
    end
})

SpeedTab:Input({
    Title = "‚úçÔ∏è Velocidade Customizada",
    Placeholder = "Ex: 100",
    Callback = function(Text)
        local val = tonumber(Text)
        if val then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = val
        end
    end
})

