--- SE√á√ÉO FAKE ---

FakeTab:Button({
    Title = "üîÆ Ativar Fake Ritual",
    Callback = function()
        local char = game.Players.LocalPlayer.Character
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if hrp then
            local plate = Instance.new("Part", workspace)
            plate.Size = Vector3.new(10, 0.5, 10)
            plate.CFrame = hrp.CFrame * CFrame.new(0, -3.5, 0)
            plate.Color = Color3.fromRGB(255, 0, 0)
            plate.Material = Enum.Material.Neon
            plate.CanCollide = false
            
            local weld = Instance.new("WeldConstraint", plate)
            weld.Part0 = plate; weld.Part1 = hrp
            
            local bv = Instance.new("BodyVelocity", hrp)
            bv.Velocity = Vector3.new(0, 60, 0)
            bv.MaxForce = Vector3.new(0, math.huge, 0)
            
            local bav = Instance.new("BodyAngularVelocity", hrp)
            bav.AngularVelocity = Vector3.new(0, 60, 0) 
            bav.MaxTorque = Vector3.new(0, math.huge, 0)
            
            task.wait(2.5)
            
            bv:Destroy()
            bav:Destroy()
            plate:Destroy()
        end
    end
})

--- SE√á√ÉO ESP ---

ESPTab:Toggle({
    Title = "üëÅÔ∏è ESP All (Highlight)",
    Default = false,
    Callback = function(Value)
        _G.ESPAll = Value
        if _G.ESPAll then
            task.spawn(function()
                while _G.ESPAll do
                    for _, v in pairs(game.Players:GetPlayers()) do
                        if v ~= game.Players.LocalPlayer and v.Character then
                            if not v.Character:FindFirstChild("RayESP") then
                                local hi = Instance.new("Highlight")
                                hi.Name = "RayESP"
                                hi.Parent = v.Character
                                hi.FillColor = Color3.fromRGB(255, 0, 0)
                                hi.OutlineColor = Color3.fromRGB(255, 255, 255)
                            end
                        end
                    end
                    task.wait(1)
                end
            end)
        else
            for _, v in pairs(game.Players:GetPlayers()) do
                if v.Character and v.Character:FindFirstChild("RayESP") then
                    v.Character.RayESP:Destroy()
                end
            end
        end
    end
})

